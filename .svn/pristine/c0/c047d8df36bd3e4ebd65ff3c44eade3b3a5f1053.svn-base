package com.tianque.plugin.account.vo;

import java.io.Serializable;

public class LedgerEducationDetail extends LedgerDetail implements Serializable {
	private Integer projectCategory;//项目类别（山坪塘、饮水工程...）
	private Integer buildType;//建设性质（新建、改建...）
	private Integer projectsubcategory;//项目类别
	private Integer distanceCategory;//路程
	private Integer roadConditionCategory;//路况
	private Double buildArea;//建设面积
	public Integer getProjectCategory() {
		return projectCategory;
	}
	public void setProjectCategory(Integer projectCategory) {
		this.projectCategory = projectCategory;
	}
	public Integer getBuildType() {
		return buildType;
	}
	public void setBuildType(Integer buildType) {
		this.buildType = buildType;
	}
	public Integer getProjectsubcategory() {
		return projectsubcategory;
	}
	public void setProjectsubcategory(Integer projectsubcategory) {
		this.projectsubcategory = projectsubcategory;
	}
	public Integer getDistanceCategory() {
		return distanceCategory;
	}
	public void setDistanceCategory(Integer distanceCategory) {
		this.distanceCategory = distanceCategory;
	}
	public Integer getRoadConditionCategory() {
		return roadConditionCategory;
	}
	public void setRoadConditionCategory(Integer roadConditionCategory) {
		this.roadConditionCategory = roadConditionCategory;
	}
	public Double getBuildArea() {
		return buildArea;
	}
	public void setBuildArea(Double buildArea) {
		this.buildArea = buildArea;
	}
	
}
