/*layout*/
body{background:#FFF;color:#333;}
.content{margin:0 5px 0 5px;background:#fff;}
#bottom{color:#fff;line-height:27px;height:27px;text-align:center;}
.ui-layout-pane {overflow: auto;background:#fff;}
.ui-layout-north,.ui-layout-south,.ui-layout-east{overflow:hidden;border:none;}
.ui-layout-west{overflow:hidden;border-top:0;}
.ui-layout-center{border:none;background:#fff;overflow:hidden;}
#tree{width:163px;height:100%; position:absolute;top:0px;right:0px;background:#fff;overflow:auto;}
.path{height:29px;line-height:29px;}
.path h1{text-indent:15px;margin-left:10px;color:#4e5e1c;}
.path h1 a{color:#4e5e1c;font-weight:normal;}
.path h1 .dq{font-weight:bold;}
.path h1 span{position:absolute;top:0px;right:20px;}
.path h1 span .zeren{_margin-top:5px;}
.column { width: 49%; float: left;padding:5px 0;}
.portlet {margin:5px 0;}
.column .portlet-header {padding-left: 0.2em;}
.portlet-header .ui-icon {float: right;}
.portlet-content { padding: 10px;}
.ui-sortable-placeholder { border: 1px dotted black; visibility: visible !important; height: 50px !important; }
/*system*/
.displayInline{display:inline-block;*display:inline:*zoom:1;}
#header{height:95px;padding:0 7px;}
#header-top{height:24px;}
#date{padding-left:10px;}
#header #version{color:#BDC3CA;}
#header-left{width:25px;height:24px;position:absolute;top:0px;left:7px;}
#header-center{color:#BDC3CA;margin:0 6px 0 25px;height:28px;line-height:28px;}
#header-center strong{color:#99FF00;font-weight:bold; font-family:Arial;}
#header-center a{color:#fff;}
#user{color:#BDC3CA;font-weight:normal;}
#header-center ul{position:absolute;top:0px;right:3px;padding-right:20px;z-index:1111;}
#header-center ul li{display:inline-block;*display:inline;*zoom:1;}
#header-center ul li#huanfu{ position:relative;}
#header-center ul li a {color:#fff;float:left;text-indent:15px;padding-right:10px;_padding:0;}
#header-center ul li a:hover{color:#003366;}
#header-center ul li a span{float:left;margin-left:8px;display:inline;cursor:pointer;}
#header-center ul li.home a:hover span,#header-center ul li.huanfu a:hover span,#header-center ul li.huanfu a:hover span{text-shadow: 2px 2px 5px #ffffff;Filter: dropshadow(color=#ffffff,offx=1,offy=1);}
#header-center ul li.view{position:relative;}
#header-center ul li.line{background:url(images/header-line.png) no-repeat right top;height:28px;width:2px;margin-left:5px;}
#header-center ul li.view span{line-height:26px;color:#fff;float:left;margin:0;background:url(images/view_button1.gif) no-repeat right 10px;padding-right:12px;cursor:pointer;}
#header-center ul li.cur span{color:#ff0;}
#header-center ul li.view strong.view-ico1{float:left;width:16px;height:28px;background:url(images/view_button.gif) no-repeat left;padding-right:5px;}
#header-center ul li.view dl{display:none;position:absolute;top:21px;left:0;background:#fff;z-index:111;}
#header-center ul li.view dl dd a{display:block;background:#3E5063;width:100px;text-align:center;text-indent:0;color:#fff;}
#header-center ul li.view dl dd a:hover{background:#536374;color:#fff;}
#header-right{position:absolute;top:0px;right:10px;width:6px;height:24px;}
#header-center ul li.pages a{background:none !important;}
#header-center ul li.pages a:hover{color:yellow !important;;}
#pagesCtt{position:absolute;top:25px;right:350px;width:500px;height:200px;z-index:11111;background:#fff;display:none;}

#skin{width:84px; position:absolute;top:23px;left:0px;z-index:1111;background:#fff;border:1px solid #1D76B4;color:#fff;display:none;}
#skin dd{float:left;width:82px;cursor: pointer;text-align:center;line-height:20px;border:1px solid #fff;}
#skin dd.default{background:#69A1DB;}
#skin dd.green{background:#99CC00;border-top:0;}
#skin dd.blue{background:#7F95C1;border-top:0;}

