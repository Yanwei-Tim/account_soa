.login dd .yz{width:97px;height:22px;line-height:22px;border:1px solid #B9B9B9;float:left;margin-top:3px;text-indent:6px;}
.login dd  img{float:left;padding-left:10px;}
.login dd a{color:#FF9900; text-decoration:underline;padding-left:10px;float:left;line-height:22px;display:inline-block;margin-top:5px;}
