.issueList li.current{background:#E6F0FB;}
.issueList li.current a{font:14px/40px "microsoft yahei";color:#336EC4;}
.issueList .showTime{padding:0 0 0 30px;}
.issueList .show{width:70px;height:16px;position:absolute;right:18px;bottom:6px;}
.issueList .showStar{background:url(/resource/js/ui/images/icon_Star.png) no-repeat;}
.issueList .oneStar{background-position:0 0;}
.issueList .twoStar{background-position:0 -17px;}
.issueList .threeStar{background-position:0 -35px;}
.issueList .fourStar{background-position:0 -53px;}
.issueList .noneStar{background-position:0 -70px;}
.issueList .handle{text-align:right;}
.issueList .show{text-align:right;}
.issueList li a.handleLink{font:12px "microsoft yahei";color:#F08301;}


.issueTitle{background:#94C2FE;height:33px;line-height:33px;color:#336EC4;overflow:hidden;}
.issueTitle .title{padding:0 0 0 10px;}
.issueTitle dl{position:relative;}
.issueTitle dt{float:left;width:110px;}
.issueTitle dt.level{}
.issueTitle .state{width:30px;padding:0 0 0 10px;background:url(/resource/js/ui/images/icon_devideLine.png) no-repeat left center;position:absolute;right:10px;}
.issueTitle .event{float:left;width:110px;text-align:center;}
.issueTitle .eventS{background:url(/resource/js/ui/images/icon_devideLine.png) no-repeat left center;}
.dealContent .timeLine{width:95px;height:200px;float:left;border:1px solid #ccc;}
.dealContent .historyContent{width:600px;height:200px;float:left;margin:0 0 0 10px;border:1px solid #ccc;}

.issueRight{padding:0 0.5%;}
.issueRight .issueReTitle{text-align:center;height:35px;font:bold 14px/35px "";color:#336EC4;}
.issueRight .recordEnd{margin:5px 0 0 0;}
.issueRight .recordEnd .mattersAssign{line-height:25px;}
.issueRight .recordEnd .mattersAssign span{margin:0 20px 0 0;}
.issueRight .recordEnd .mattersAssign .place{font-weight:bold;color:#346FC5;}
.issueRight .recordEnd h3{height:25px;line-height:25px;border-bottom:1px solid #336EC4;}
.issueRight .dealRecord h3{height:25px;line-height:25px;border-bottom:1px solid #336EC4;}

.issueRight .smallText{margin:10px 0 0 0;line-height:20px;}
.issueRight .smallText .instruction{text-indent:30px;}
.issueRight .processInstruct{position:relative;margin:10px 0 0 0;padding:0 0 0 115px;}
.issueRight .recordList .timerShaft{width:95px;height:400px;padding:40px 0 0 0;position:absolute;top:0px;left:5px;}
.issueRight .recordList .timerShaft a{height:20px;display:block;line-height:20px;margin:0 0 2px 0;color:#5F5F5F;position:relative;}
.issueRight .recordList .timerShaft a.button{width:18px;height:11px;margin:5px 0 0 35px;color:#5F5F5F;background:url(/resource/js/ui/images/icon_updown.png) no-repeat;text-indent:-9999em;}
.issueRight .recordList .timerShaft a.up{background-position:0 0;}
.issueRight .recordList .timerShaft a.down{background-position:0 -38px;}
.issueRight .recordList .timerShaft a span{width:7px;height:20px;display:block;background:#ccc;position:absolute;right:0px;top:0px;}
.issueRight .recordList .timerShaft a.current span{background:#FF6600;}

.issueRight .recordList .timerShaft .timerShaftC{height:110px;overflow:hidden;}

.issueRight .recordList .recordContent{border:1px solid #9A9A9A;}
.issueRight .recordList .recordContent ul{height:265px;overflow:auto;}
.issueRight .recordList .recordData{height:27px;line-height:27px;padding:0 0 0 10px;background:#E6F0FB;position:relative;}
.issueRight .recordData .time,
.issueRight .recordData .place,
.issueRight .recordData .handle{float:left;padding:0 10px;color:#5F5F5F;}
.issueRight .recordData .time{background:url(/resource/js/ui/images/icon_devideLine.png) no-repeat right center;}
.issueRight .recordData .place{font:bold 12px/27px "";color:#346FC5;}
.issueRight .recordData .handle{font:bold 12px "";color:#4D4D4D;position:absolute;right:8px;top:5px;}
.issueRight .recordData .handle a{font:bold 12px "";color:#F08402;}
.issueRight .smallText{text-indent:30px;}
/* table */
.newTableList{width:100%;border-width: 1px;border-style:solid;border-color: #D5D4D4; border-collapse: separate;background:#EBEBEB;border-top:1px solid #fff;border-right:1px solid #fff;}
.newTableList tr.odd{background:#EEE;}
.newTableList tr.even{background:#E6E6E6;}
.newTableList td{height:18px;padding:0 0 0 10px;border-color: #D5D4D4 #D5D4D4 #fff #F8F8F8; border-width: 1px;border-style: solid;border-collapse:collapse;line-height:20px;word-break:break-all;word-warp:break-word;}
.newTableList .title{width:12%;background:#ECF1F8;font:normal 12px/20px "microsoft yahei";color:#000;padding:0 0 0 5px;}
.newTableList .text{width:25%;}
.hidden{display:none;}

/*pagination*/
div.issuePagination{font-family:Verdana,Tahoma,Arial,Helvetica,Sans-Serif;text-align:center;padding:4px 6px 4px 0;	background-color:#fff;	color:#313031;line-height:25px;overflow:hidden;}
div.issuePagination em{font-style:normal;}
div.issuePagination a{color:#5F5F5F;text-decoration:none;padding:1px 5px 2px 5px;margin:0 3px 0 3px;border:1px solid #EFEFEF;font-family:"microsoft yahei";}
div.issuePagination a:hover, div.issuePagination a:active {color:#0066a7;border:1px solid #b7d8ee;background-color:#d2eaf6;}
div.issuePagination span.current {padding:1px 5px 2px 5px;margin:0 3px 0 3px;border:1px solid #B5CDEF;font-weight:bold;color:#FFF;background-color:#336EC4;}
div.issuePagination span.disabled {display:none;}
div.issuePagination .prev,div.issuePagination .next{white-space:nowrap;}
div.issuePagination .prev{background:url(/resource/js/ui/images/icon_pagePre.png) no-repeat 2px center;padding-left:8px;}
div.issuePagination .next{background:url(/resource/js/ui/images/icon_pageNext.png) no-repeat 45px center;padding-right:10px;}
