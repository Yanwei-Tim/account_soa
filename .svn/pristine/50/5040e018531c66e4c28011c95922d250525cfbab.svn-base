/*version update*/
.focusBox { position: absolute; width: 815px; height: 614px;top:50%;left:50%;margin:-307px 0 0 -407px;overflow: hidden;z-index:1111;}
.focusBox .innerBox{position:relative;}
.focusBox .pic { position: relative; z-index: 0; }
.focusBox .pic li{background:url(../images/version/version_bg.png) no-repeat;padding:85px 30px 0 30px;width:730px;height:554px;overflow:hidden;}
.focusBox .pic img {display: block; }
.focusBox .pic li .step{height:170px;float:left;}
.focusBox .pic li .hover{background:url(../images/version/version_li_hover.png) repeat;}
.focusBox .pic li .right{float:right;padding-right:30px;}
.focusBox .hd { width: 500px; position: absolute;left: 50%;bottom: 45px;margin-left: -250px;text-align: center; font-size: 0; z-index: 1; }
.focusBox .hd li { margin: 0 5px; background: url(../images/version/dot.png) no-repeat 0 -22px; height: 18px; overflow: hidden; width: 18px; cursor: pointer; display: inline-block; *display:inline;zoom: 1; _background: url(../images/version/dot8.png) no-repeat -1px -22px; }
.focusBox .hd .on { background-position: -1px -1px; }
.focusBox .prev, .focusBox .next { width: 60px; height: 90px; margin: -60px 0 0; display: none; background: url(../images/version/ad_ctr.png) no-repeat 0 0; position: absolute; top: 50%; z-index: 10; cursor: pointer; text-indent: -9999px; filter: alpha(opacity=20); opacity: 0.2; }
.focusBox .prev { left: 7px; }
.focusBox .next { background-position: 0 -90px; right:26px; }
.focusBox .prev:hover, .focusBox .next:hover { filter: alpha(opacity=50) !important; opacity: 0.5 !important;}
.focusBox .close{z-index: 1;position: absolute;top: 15px;right: 30px;text-indent:-9999em;background:url(../images/version/close.png) no-repeat;display:block;width:21px;height:21px;}
.focusBox .stepBtn{position: absolute;bottom: 45px;right: 30px;z-index:1;text-indent:-9999em;background:url(../images/version/step_btn.png) no-repeat;display:block;width:124px;height:41px;}
.version-overlay{position: absolute;top: 0;left: 0;width: 100%;height: 100%;background:#000;opacity:.4;filter: Alpha(Opacity=40);z-index:111;}