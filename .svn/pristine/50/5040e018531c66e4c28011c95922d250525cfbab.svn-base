.login-window { position: absolute; top: 16px; right: 150px; width: 270px; color: white; line-height: 1em; }
.login-window .pic {cursor:pointer;display: inline-block; *zoom: 1; *display: inline; margin-right: 6px; border: 1px solid #fff; float: left; }
.login-window .pic img { width: 35px; height: 35px; }
.login-window .cont { position:relative; margin-left: 43px; font-family: microsoft YaHei; }
.login-window .cont .personal { display: block; margin-bottom: 5px; }
.login-window .cont a {color: #fff;padding:0 5px;}
.login-window .cont a:hover {}
.login-window .cont .switchFunction{position:relative;}
.login-window .cont span.loopSkin,
.login-window .cont span.loopTypeface,
.login-window .cont span.separator {
display: inline-block; *zoom: 1; *display: inline; background: url("../images/triangle.png") no-repeat scroll right center transparent; padding-right: 12px; cursor:pointer;
border:1px solid transparent;
padding: 4px 18px 4px 4px;
}
.login-window .cont span.separator{width:1px; background:none; padding:4px 0;}
.login-window .cont span.loopSkin ,.login-window .cont span.separator { margin-right: 5px; }
.login-window .cont span.cur,.login-window .loopSkinCont{position:absolute; left:0; top:0; border: 1px solid #D6D6D6; background-color:#FFF;}
.login-window .cont span.cur{position: relative;z-index:2;color: black;border: 1px solid #D6D6D6;border-bottom:0 none;background: white url("../images/triangle_black.png") no-repeat right 8px;}
.login-window .loopSkinCont{float:left;position: absolute;left: 0;top: 39px;width:70px;line-height:24px;padding:2px;}
.login-window  .loopSkinCont a{color:#000;display:block;text-decoration:none;padding-left:4px;}
.login-window  .loopSkinCont a:hover{background-color:#dceaff;}
.login-window  .loopSkinCont span{display:inline-block;*display:inline;*zoom:1;width: 12px;height: 12px;vertical-align: middle;margin:0 3px;}
.login-window  .loopSkinCont span.blue-color{background:#4B87D4;}
.login-window  .loopSkinCont span.red-color{background:#B71A1B;}
.login-window  .loopSkinCont span.green-color{background:#009B9E;}
.login-window  #loopTypeFaceCont{width:80px;}

.switchFunction .fli{float:left;display:inline-block;*display:inline;*zoom:1;margin: 0 10px 0 0;line-height:23px;position:relative;}
.switchFunction span{cursor:pointer;}
.switchFunction .functionInfo{width:75px;height:23px;display:inline-block;*display:inline;*zoom:1;padding:0 0 0 10px;background:url(../images/icon_sarrow.png) no-repeat 90% -6px;_background-color:none;_background-image:url(../images/icon_sarrow.png) no-repeat 90% -6px;}
.switchFunction .functionInfo.backCur{color:#3B3B3B;background:#fff url(../images/icon_sarrow.png) no-repeat 90% 8px;_background-color:#fff;_background-image:url(../images/icon_sarrow.png) no-repeat 90% 8px;}
.functionCont{position:absolute;left:0px;top:23px;overflow:visible!important;}
.functionCont li.func{width:70px;padding:0 0 0 15px;}
.functionCont li{text-indent:12px;height:23px;line-height:23px;font:12px/23px "";color:#333;background:#fff;position:relative;}
.functionContList li{height:23px;}
.functionCont li.funcCurrent,.functionCont li:hover{background:#E6E7E7;}
.login-window .cont .functionCont li a{font:12px/23px "";color:#333;}
.functionCont .list{width:100px;position:absolute;left:85px;top:0px;}
.functionCont .list li{text-indent:5px;}
.functionCont .setC{float:left;width:16px;display:inline-block;*display:inline;*zoom:1;background:url(../images/funcTools.png) no-repeat;position:absolute;left:10px;top:2px;}
.functionCont .skinC{height:13px;background-position:0 2px;}
.functionCont .fontC{height:23px;background-position:0 -23px;}
.functionCont .list a{display:inline-block;*display:inline;*zoom:1;}
.functionCont .list a span{width:10px;height:10px;display:inline-block;*display:inline;*zoom:1;margin:0 5px 0 0;overflow:hidden;}
.functionCont .list a span.blue-color{background:#4B87D4;}
.functionCont .list a span.red-color{background:#B71A1B;}
.functionCont .list a span.green-color{background:#009B9E;}

