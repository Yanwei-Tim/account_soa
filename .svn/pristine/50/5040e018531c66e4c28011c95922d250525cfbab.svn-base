/*common*/
.displayInline{display:inline-block; *display:inline:*zoom:1;}
/*layout*/
.ui-layout-pane{overflow:auto; background:#f4f5f5;}
.ui-layout-north{position:relative;z-index:11}
.ui-layout-north,.ui-layout-south,.ui-layout-east{border:none;}
.ui-layout-west{border-right:1px solid #d6d6d6;overflow:hidden; border-top:0; float:left; width:198px; height:600px;background:#F4F5F5;position:relative;}

.slideResizer{width:8px;height: 460px;float:left;}
.slideResizer .slideToggler{width:8px;height:47px;background:url(../images/slideToggler.jpg) no-repeat 0 -47px;margin-top:200px;cursor:pointer;margin-left:-1px;}
.slideResizer .slideTogglerCur{background-position:0 0;}

.ui-layout-west{overflow:auto; margin-top:5px;}
.ui-layout-center{border:none; background:#f4f5f5; position:relative;overflow:hidden;}
.ui-layout-south{position:fixed; _position:absolute; bottom:0; left:0; z-index:3; width:100%; background:#2e2e2e;height:30px;}
.ui-layout-north{height:70px;}
#contentDiv{padding-left:5px;}
#bottom{color:#fff; height:30px; line-height:30px; text-align:center;width:100%;text-align:center;color:#fff;background:#2E2E2E;position:fixed;bottom:0px;_position:absolute;_bottom:auto;_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)));}  
#bottom a{color:#fff;}
#bottom span{display:inline-block;*display:inline;*zoom:1;margin:0 10px;}
